{{if .Sponsors.Valid}}
  <div id="sponsors">
      <div class="glide">
      <div class="glide__track" data-glide-el="track">
        <ul class="glide__slides">
          {{range .Sponsors.List}}
          <li class="glide__slide">
            <img src="{{.}}">
          </li>
          {{end}}
        </ul>
      </div>
    </div>
  </div>
  <style type="text/css">
    .glide img{
      height:120px;
    }
    .glide__slide{
      background:rgba(200,200,200,.3);
      padding:4px;
      text-align: center;
      border-radius: 8px;
    }
    .glide__slides>.glide__slide:nth-child(even){
      background: rgba(55,55,55,.66);
    }
    .glide{
      max-width:650px;
      margin:90px auto 60px;
      overflow: hidden;
    }
  </style>
  <script>
  new Glide('.glide', {
    type: 'carousel',
    startAt: 0,
    perView: 3
  }).mount();
  </script>
{{end}}
<div class='flex-row flex-wrap' id="widget">

  {{if .Widget.Valid}}
    <div class='flex-column flex-grow'>
     {{.Widget.String}}
    </div>
  {{end}}

  {{if or .Contact.address .Contact.phone .Contact.whatsapp .Contact.email}}
  <div class='contact-other flex-column flex-grow'>
    <h1 class='con-title' id="contacto">Contactanos</h1>
    <ul class='con-list'>
    {{if .Contact.address}}<li><i class='lnr lnr-map'></i>{{.Contact.address}}</li>{{end}}
    {{if .Contact.phone}}<li><i class='lnr lnr-phone'></i>{{.Contact.phone}}</li>{{end}}
    {{if .Contact.whatsapp}}<li><i class='lnr lnr-phone'></i>{{.Contact.whatsapp}}</li>{{end}}
    {{if .Contact.email}}<li><i class='lnr lnr-envelope'></i>{{.Contact.email}}</li>{{end}}
